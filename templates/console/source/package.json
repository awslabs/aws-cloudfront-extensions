{
  "name": "console",
  "version": "0.1.0",
  "bin": {
    "console": "bin/console.js"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run test && npm run synth",
    "bump": "standard-version -i ../CHANGELOG.md",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:compile": "tsc --noEmit --project tsconfig.jest.json",
    "watch": "tsc -w",
    "eslint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test",
    "cdk": "cdk",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.23.0",
    "constructs": "10.0.110"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.33",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "aws-cdk-lib": "2.23.0",
    "constructs": "10.0.110",
    "eslint": "^7.32.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^26.6.3",
    "jest-junit": "^12",
    "json-schema": "^0.4.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^26.5.6",
    "ts-node": "10.4.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.23.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.23.0-alpha.0",
    "@aws-cdk/aws-appsync-alpha": "2.23.0-alpha.0",
    "@aws-solutions-constructs/aws-cloudfront-s3": "^2.8.0",
    "aws-cdk-lib": "2.23.0",
    "cdk-bootstrapless-synthesizer": "2.2.2",
    "constructs": "10.0.110"
  },
  "bundledDependencies": [],
  "license": "Apache-2.0",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "jest.config.js"
      }
    }
  },
  "bundleDependencies": []
}
