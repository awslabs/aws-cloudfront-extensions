{
  "name": "cloudfront-extensions-cdk",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "build": "npm run test && npm run synth",
    "bump": "standard-version -i ../CHANGELOG.md",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:compile": "tsc --noEmit --project tsconfig.jest.json",
    "watch": "tsc -w",
    "eslint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test",
    "cdk": "cdk",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.133.0",
    "constructs": "10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk-lib": "2.133.0",
    "constructs": "10.3.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.133.0",
    "cdk-assets": "^2.131.0",
    "cdk-bootstrapless-synthesizer": "2.2.2",
    "constructs": "10.3.0",
    "source-map-support": "^0.5.16"
  }
}
