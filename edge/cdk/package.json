{
  "name": "cloudfront-extensions-cdk",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "build": "npm run test && npm run synth",
    "bump": "standard-version -i ../CHANGELOG.md",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:compile": "tsc --noEmit --project tsconfig.jest.json",
    "watch": "tsc -w",
    "eslint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test",
    "cdk": "cdk",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.20.0",
    "constructs": "10.0.36"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.128.0",
    "@aws-cdk/cloud-assembly-schema": "^2.18.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk-lib": "2.20.0",
    "constructs": "10.0.36",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.20.0",
    "cdk-bootstrapless-synthesizer": "2.2.2",
    "constructs": "10.0.36",
    "source-map-support": "^0.5.16",
    "@types/uuid": "^8.3.4"
  }
}
